{% extends 'base.html.twig' %}

{% block title %}HistorialSeleccion{% endblock %}

{% block body %}
    <h1>HistorialSeleccion</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ historial_seleccion.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ historial_seleccion.fecha ? historial_seleccion.fecha|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Torneo</th>
                <td>{{ historial_seleccion.torneo }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ historial_seleccion.activo ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>FechaCreacion</th>
                <td>{{ historial_seleccion.fechaCreacion ? historial_seleccion.fechaCreacion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FechaActualizacion</th>
                <td>{{ historial_seleccion.fechaActualizacion ? historial_seleccion.fechaActualizacion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('historial_seleccion_index') }}">back to list</a>

    <a href="{{ path('historial_seleccion_edit', {'id': historial_seleccion.id}) }}">edit</a>

    {{ include('historial_seleccion/_delete_form.html.twig') }}
{% endblock %}