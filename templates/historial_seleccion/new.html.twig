{% extends 'base.html.twig' %}

{% block title %}Nuevo Historial Selección{% endblock %}

{% block body %}
    <h1>Nuevo Historial Selección</h1>

    <div class="row">
        <div class="col-md-6">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">{{ historial_seleccion.jugador }}</h3>
                </div>
                <div class="card-body">
                    {{ include('historial_seleccion/_form.html.twig') }}
                </div>
            </div>
        </div>
    </div>
    <a class="btn btn-default" href="{{ path('jugador_show', {'id': historial_seleccion.jugador.id }) }}">Volver</a>
{% endblock %}