{% extends 'base.html.twig' %}

{% block title %}FichaMedica{% endblock %}

{% block body %}
    <h1>FichaMedica</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ficha_medica.id }}</td>
            </tr>
            <tr>
                <th>Prestador</th>
                <td>{{ ficha_medica.prestador }}</td>
            </tr>
            <tr>
                <th>NumeroAfiliado</th>
                <td>{{ ficha_medica.numeroAfiliado }}</td>
            </tr>
            <tr>
                <th>TieneCobertura</th>
                <td>{{ ficha_medica.tieneCobertura ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Doctor</th>
                <td>{{ ficha_medica.doctor }}</td>
            </tr>
            <tr>
                <th>Matricula</th>
                <td>{{ ficha_medica.matricula }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ ficha_medica.activo ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>FechaCreacion</th>
                <td>{{ ficha_medica.fechaCreacion ? ficha_medica.fechaCreacion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FechaActualizacion</th>
                <td>{{ ficha_medica.fechaActualizacion ? ficha_medica.fechaActualizacion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ficha_medica_index') }}">back to list</a>

    <a href="{{ path('ficha_medica_edit', {'id': ficha_medica.id}) }}">edit</a>

    {{ include('ficha_medica/_delete_form.html.twig') }}
{% endblock %}