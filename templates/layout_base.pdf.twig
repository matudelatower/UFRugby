<!doctype html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="icon" type="image/x-icon" href="{{ absolute_url(asset('build/img/logo_fbr.jpg')) }}"/>
    <title>{{ title }}</title>

    <style>

        body{
            font-size: 14pt;
        }
        table {
            border-collapse: collapse;
        }

        table, th {
            border: 2px solid black;
        }

        table, td {
            border: 1px solid black;
        }

        .titulo-reporte {
            text-align: center;
        }
    </style>
    {% block stylesheets %}
    {% endblock stylesheets %}
</head>
<body>
<h2 class="titulo-reporte">{{ title }}</h2>
{% block body %}
{% endblock %}

</body>
</html>