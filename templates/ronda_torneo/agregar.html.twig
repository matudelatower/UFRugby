{% extends 'base.html.twig' %}

{% block title %}Agregar Ronda{% endblock %}

{% block body %}
    <h1>Ronda</h1>
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">Torneo: {{ ronda_torneo.torneo }}</h3>
        </div>
        <div class="box-body">
            {{ include('ronda_torneo/_form.html.twig') }}
        </div>
    </div>
    <a class="btn btn-default" href="{{ path('torneo_show', {'id': ronda_torneo.torneo.id}) }}">
        Volver
    </a>
{% endblock %}